{
  "metadata": {
    "name": "java"
  },
  "projects": [
    {
      "name": "camel-spring-boot",
      "source": {
        "location": "https://github.com/mmistretta/camel-spring-boot.git",
        "type": "git"
      }
    },
  ],
  "components": [
    {
      "id": "redhat/java8/latest",
      "type": "chePlugin"
    },
    {
      "id": "redhat/vscode-apache-camel/latest",
      "type": "chePlugin"
    },
    {
      "reference": "https://raw.githubusercontent.com/eclipse/che-plugin-registry/master/v3/plugins/redhat/vscode-camelk/0.0.12/meta.yaml",
      "type": "chePlugin"
    },
    {
      "id": "redhat/vscode-xml/latest",
      "type": "chePlugin"
    },
    {
      "mountSources": true,
      "endpoints": [
        {
          "name": "8080",
          "port": 8080
        }
      ],
      "memoryLimit": "768Mi",
      "type": "dockerimage",
      "volumes": [
        {
          "name": "m2",
          "containerPath": "/home/jboss/.m2"
        }
      ],
      "alias": "maven",
      "image": "registry.redhat.io/codeready-workspaces/stacks-java-rhel8:2.0",
      "env": [
        {
          "value": "-XX:MaxRAMPercentage=50.0 -XX:+UseParallelGC -XX:MinHeapFreeRatio=10 -XX:MaxHeapFreeRatio=20 -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xms20m -Djava.security.egd=file:/dev/./urandom -Duser.home=/home/jboss",
          "name": "JAVA_OPTS"
        },
        {
          "value": "$(JAVA_OPTS)",
          "name": "MAVEN_OPTS"
        }
      ]
    }
  ],
  "apiVersion": "1.0.0"
}
